        "libs/board-game-store/feature-cart/src/index.ts"
      ],
      "@stores/board-game-store/ui-formatters": [
        "libs/board-game-store/ui-formatters/src/index.ts"
      ],
      "@stores/board-game-store/data-access-games": [
        "libs/board-game-store/data-access-games/src/index.ts"
      ],
      "@stores/store/data-access-cart": [
        "libs/store/data-access-cart/src/index.ts"
      ],
      "@prb/my-plugin": ["libs/plugins/my-plugin/src/index.ts"],
      "@prb/my-plugin-next": ["libs/my-plugin-next/src/index.ts"],
      "@prb/aws-cdk": ["libs/aws-cdk/src/index.ts"],
